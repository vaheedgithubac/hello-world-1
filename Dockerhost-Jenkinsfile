pipeline {
    agent any
   
    options {
        timestamps()
    }
    
    tools {
        maven 'Maven-363'
    }
    
    
    stages {
    
        stage('Maven Build') {
            steps {
                sh 'mvn clean install package'
            }
        } // stage
        
        stage() {
        
             steps {
                sshagent(['Docker-Host-Creds']) {
                       // some block
                       sh 'scp -o StrictHostKeyChecking=no webapp/target/webapp.war dockeradmin@172.31.43.177:/opt/docker-workspace'
                }
              }
        } // stage
        
     }
